{
  "env": {
    "development": {
      "presets": ["es2015", "react"],
      "plugins": [
        "syntax-object-rest-spread",
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    },
    "test": {
      "presets": ["es2015", "react"],
      "plugins": [
        "syntax-object-rest-spread",
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    },
    "production": {
      "presets": ["es2015-rollup", "react"],
      "plugins": [
        "syntax-object-rest-spread",
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    }
  }
}
